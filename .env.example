# Aurelius Web Dashboard Environment Variables
# Copy this file to .env.local and fill in your actual values

# App Configuration
NEXT_PUBLIC_APP_NAME=Aurelius
NEXT_PUBLIC_APP_DESCRIPTION=Your Personal IMVU Modeling Assistant
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Discord Bot Configuration (for invite link)
NEXT_PUBLIC_DISCORD_CLIENT_ID=your_discord_client_id_here

# Optional: Default Supabase Configuration (for demo purposes)
# Users can override this with their own database
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
